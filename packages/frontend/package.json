{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "7.19.1",
    "@babel/preset-env": "7.19.1",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@mui/icons-material": "5.10.6",
    "@mui/material": "5.10.6",
    "@sentry/react": "7.12.1",
    "@sentry/tracing": "7.12.1",
    "axios": "0.27.2",
    "babel-loader": "8.2.5",
    "html-webpack-plugin": "5.5.0",
    "path": "0.12.7",
    "psl": "1.9.0",
    "ramda": "0.28.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.4.0",
    "react-scripts": "5.0.1",
    "styled-components": "5.3.5",
    "typescript": "4.8.3",
    "validator": "13.7.0",
    "web-vitals": "3.0.2",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "CI=true react-scripts test",
    "test:coverage": "CI=true react-scripts test --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint -c .eslintrc 'src/**/*.{ts,tsx}'",
    "type-check": "npx tsc --noEmit",
    "precommit": "yarn type-check && yarn lint && yarn test:ci",
    "serve": "webpack serve --color --progress --config webpack/webpack.js",
    "release": "webpack --color --progress --config webpack/webpack.js",
    "dev": "NODE_ENV=development yarn serve --open"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.19.1",
    "@babel/preset-env": "7.19.1",
    "@badeball/cypress-cucumber-preprocessor": "13.0.2",
    "@bahmutov/cypress-esbuild-preprocessor": "2.1.4",
    "@cypress/webpack-preprocessor": "5.12.2",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.7",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "13.5.0",
    "@types/axios": "0.14.0",
    "@types/cypress-cucumber-preprocessor": "4.0.1",
    "@types/enzyme": "3.10.12",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "29.0.3",
    "@types/node": "18.7.18",
    "@types/prettier": "2.7.0",
    "@types/psl": "1.1.0",
    "@types/ramda": "0.28.15",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@types/styled-components": "5.1.26",
    "@types/validator": "13.7.6",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "babel-loader": "8.2.5",
    "cypress": "10.8.0",
    "cypress-cucumber-preprocessor": "4.3.1",
    "cypress-real-events": "1.7.1",
    "cypress-wait-until": "1.7.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "8.23.1",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-prettier": "4.2.1",
    "flush-promises": "1.0.2",
    "jest": "29.0.3",
    "msw": "0.47.1",
    "prettier": "2.7.1",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.11.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.tsx",
      "!src/App.tsx",
      "!src/global.d.ts",
      "!src/reportWebVitals.ts",
      "!src/styles/**/*.ts",
      "!src/**/*.d.ts"
    ],
    "coveragePathIgnorePatterns": [
      "lib/messages.ts"
    ]
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "moduleResolution": "node",
    "stepDefinitions": "cypress/integration/common/**/*.ts"
  }
}
